<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SeaPorts" table="sea_ports">
    <indexes>
      <index name="IDX_28889FC285478676" columns="town_city_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="sea_ports_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="name" type="string" column="name" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="townCity" target-entity="TownCities" fetch="LAZY">
      <join-columns>
        <join-column name="town_city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
