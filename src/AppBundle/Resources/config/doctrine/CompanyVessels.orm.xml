<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CompanyVessels" table="company_vessels">
    <indexes>
      <index name="IDX_E31F26DA73094C3D" columns="arrive_port_id"/>
      <index name="IDX_E31F26DA979B1AD6" columns="company_id"/>
      <index name="IDX_E31F26DADA760B40" columns="depart_port_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="company_vessels_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="vesselType" type="string" column="vessel_type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departTime" type="string" column="depart_time" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="arriveTime" type="string" column="arrive_time" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="155" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="schedDay" type="string" column="sched_day" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passPriceRange" type="string" column="pass_price_range" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vehiPriceRange" type="string" column="vehi_price_range" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="arrivePort" target-entity="SeaPorts" fetch="LAZY">
      <join-columns>
        <join-column name="arrive_port_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="company" target-entity="Companies" fetch="LAZY">
      <join-columns>
        <join-column name="company_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="departPort" target-entity="SeaPorts" fetch="LAZY">
      <join-columns>
        <join-column name="depart_port_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
