<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CompanyBookings" table="company_bookings">
    <indexes>
      <index name="IDX_62122958979B1AD6" columns="company_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="company_bookings_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="address" type="string" column="address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="company" target-entity="Companies" fetch="LAZY">
      <join-columns>
        <join-column name="company_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
