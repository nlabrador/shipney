<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\VesselAccomodations" table="vessel_accomodations">
    <indexes>
      <index name="IDX_76FE33F814AF1953" columns="vessel_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="vessel_accomodations_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="accomodation" type="string" column="accomodation" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="price" type="decimal" column="price" precision="10" scale="0" nullable="true"/>
    <field name="features" type="text" column="features" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="vessel" target-entity="CompanyVessels" fetch="LAZY">
      <join-columns>
        <join-column name="vessel_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
